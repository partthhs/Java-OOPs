class Animal {
void eat() 
{
System.out.println("Eating.....");
}
 }

class Dog extends Animal {
void bark() {
System.out.println("Barking...");
  }
}

class Puppy extends Dog {
void weep() {
System.out.println("Weeping.....");
  }
}

public class Main {
public static void main(String[]args)
{
Puppy p1 = new Puppy();

p1.eat();
p1.bark();
p1.weep();
 
 }
}